        "3 value input from user"
      ],
      "metadata": {
        "id": "uAueGqPMPdSa"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "a = int(input(\"Enter the value for a \"))\n",
        "b = int(input(\"Enter the value for b \"))\n",
        "c = int(input(\"Enter the value for c from 1 - 6 2\"))\n",
        "if(c==1):\n",
        "  d=a+b\n",
        "  print(f\"The choice is {c} and the output is a{a} + b{b} = {d} \")\n",
        "elif(c==2):\n",
        "  d=a-b\n",
        "  print(f\"The choice is {c} and the output is a{a} - b{b} = {d} \")\n",
        "elif(c==3):\n",
        "  d=a*b\n",
        "  print(f\"The choice is {c} and the output is a{a} * b{b} = {d} \")\n",
        "elif(c==4):\n",
        "  if(a==0 or b ==0):\n",
        "    print(\"This gives error dont try dividing by 0 \")\n",
        "  else:\n",
        "    d=a/b\n",
        "    print(f\"The choice is {c} and the output is a{a} / b{b} = {d} \")\n",
        "elif(c==5):\n",
        "  d=a%b\n",
        "  print(f\"The choice is {c} and the output is a{a} % b{b} = {d} \")\n",
        "elif(c==6):\n",
        "  d=a**b\n",
        "  print(f\"The choice is {c} and the output is a{a} ** b{b} = {d} \")\n",
        "else :\n",
        "  print(\"Print incorrect value \")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "xDbnsrdvPXQP",
        "outputId": "afe04929-1ac8-45b5-953c-1f969223c983"
