        "a = int(input(\"Enter the value for a \"))\n",
        "b = int(input(\"Enter the value for b \"))\n",
        "c = int(input(\"Enter the value for c \"))\n",
        "\n",
        "if a==b and b==c :\n",
        "  print(\"All are Equal\")\n",
        "elif (a>b):\n",
        "  if (a>c):\n",
        "    print(\"a is greater\")\n",
        "else :\n",
        "  if(b>a):\n",
        "    print(\"B is greater\")\n",
        "  else:\n",
        "    print(\"C is greater\")"
